{
  "cli": {
    "version": ">= 2.6.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "store",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "store"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "development": {
      "android": {
        "serviceAccountKeyPath": "./googleKeys/SERVICE_ACCOUNT_GOOGLE_KEY",
        "track": "production"
      }
    }
  }
}
